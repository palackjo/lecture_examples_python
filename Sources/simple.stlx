a := { 1, 2, 3 };
b := { 2, 3, 4 };
// computing the union 
c := a + b;
print(a, " + ", b, " = ", c);
// computing the intersection 
c := a * b;
print(a, " * ", b, " = ", c);
// computing the set difference 
c := a - b;
print(a, " - ", b, " = ", c);
// computing the power set
c := pow(a);
print("pow(", a, ") = ", c);
// testing the inclusion relation 
print("(", a, " <= ", b, ") = ", (a <= b)); 

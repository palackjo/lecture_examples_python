suits  := { "c", "h", "d", "s" };
values := { "2", "3", "4", "5", "6", "7", "8", "9", 
            "T", "J", "Q", "K", "A" }; 
deck   := { [ v, s ] : v in values, s in suits };
hole   := { [ "3", "c" ], [ "3", "s" ] };
rest   := deck - hole;
flops  := { { k1, k2, k3 } : k1 in rest, k2 in rest, k3 in rest 
                           | #{ k1, k2, k3 } == 3 };
print(#flops);
trips  := { f : f in flops | [ "3", "d" ] in f || [ "3", "h" ] in f };
print(1.0 * #trips / #flops);

